<h3>Cadastrar Tipo de Despesa</h3>

<%= form_for(@expense_type) do |f| %>
  <div>
    <%= f.label :description, 'Descrição' %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.label :accounting_account, 'Conta Contábil' %>
    <%= f.text_field :accounting_account %>
  </div>
  <%= f.submit 'Cadastrar' %>
<% end %>
